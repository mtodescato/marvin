{
  "name": "poc",
  "version": "0.0.1",
  "description": "proof of concept",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint .",
    "test": "NODE_ENV=development mocha --require babel-core/register --require ignore-styles --require ./src/utils/testSetup.js \"src/test/**/*.test.js\" --exit",
    "test-windows": "set NODE_ENV=development&&mocha --require babel-core/register --require ignore-styles --require ./src/utils/testSetup.js \"src/test/**/*.test.js\" --exit",
    "reactscripts-test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "start-all": "npm-run-all -p watch-css ganache-script truffle-script start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "truffle-script": "cd ./src/truffle/ && truffle compile --all && truffle migrate --reset --network development",
    "ganache-script": "./node_modules/.bin/ganache-cli -p 7545 -m velociraptor"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  },
  "author": "NOVEGroup",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "eslint": "^4.18.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-mocha": "^4.12.1",
    "eslint-plugin-react": "^7.7.0",
    "solhint": "^1.1.10"
  },
  "dependencies": {
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "espower-typescript": "^8.1.3",
    "ganache-cli": "^6.1.0",
    "grommet": "^1.10.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.6.2",
    "mocha": "^5.0.4",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-router-prop-types": "0.0.3",
    "react-scripts": "^1.1.1",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "redux-saga-testing": "^1.0.5",
    "reselect": "^3.0.1",
    "truffle-contract": "^3.0.4",
    "web3": "^0.20.0"
  }
}
